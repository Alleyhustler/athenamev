        function showTab(tabName) {
            var tabs = ['home', 'about', 'gallery', 'contact'];
            tabs.forEach(function(tab) {
                document.getElementById(tab).style.display = 'none';
            });
            document.getElementById(tabName).style.display = 'block';
        }
